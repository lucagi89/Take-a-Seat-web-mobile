{
  "name": "take-a-seat",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:mobile": "turbo run start --filter=mobile",
    "dev:mobile-test": "turbo run start --filter=mobile-test",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "turbo": "^2.5.3",
    "prettier": "^3.5.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@11.1.0",
  "engines": {
    "node": ">=18"
  }
}
